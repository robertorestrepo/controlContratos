{% extends 'base.html.twig' %}

{% block body %}
    <h1>TipoEntidad</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ tipoEntidad.id }}</td>
            </tr>
            <tr>
                <th>Tipo</th>
                <td>{{ tipoEntidad.tipo }}</td>
            </tr>
            <tr>
                <th>Idtipo</th>
                <td>{{ tipoEntidad.idTipo }}</td>
            </tr>
            <tr>
                <th>Current</th>
                <td>{% if tipoEntidad.current %}{{ tipoEntidad.current|date('Y-m-d H:i:s') }}{% endif %}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('tipoentidad_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('tipoentidad_edit', { 'id': tipoEntidad.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
